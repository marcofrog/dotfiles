colorscheme habamax
set number
syntax enable
let mapleader = ' '
let maplocalleader = ' '
let netrw_keepdir = 0
noremap <leader>e :Ex<CR>
noremap <leader>y "+y
noremap <S-Up> 10<Up>
noremap <S-Down> 10<Down>
noremap <S-Left> <Home>
noremap <S-Right> <End>
set ignorecase
set belloff=all
noremap <C-s> :w<CR>
imap <C-s> <ESC>:w<CR>
set mouse=a
